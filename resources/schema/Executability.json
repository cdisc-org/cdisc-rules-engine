{
  "$id": "Executability.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "oneOf": [
    {
      "const": "Fully Executable"
    },
    {
      "const": "Not Executable"
    },
    {
      "const": "Partially Executable"
    },
    {
